<script>
  $('.modalBTN').click(function () {
    //user modal
    let userId = $(this).data('id');
    let routeURL = '/admin/userStatus/' + userId
    $(".modal-footer #confirmBTN").attr('href', routeURL);

    //category  modal
    let categoryId = $(this).data('id');
    let catrouteURL = '/admin/categoryStatus/' + categoryId
    $(".modal-footer #catconfirmBTN").attr('href', catrouteURL);

    //product modal
    let productId = $(this).data('id');
    let prodrouteURL = '/admin/productStatus/' + productId
    $(".modal-footer #prodConfirmBTN").attr('href', prodrouteURL);

    //coupon modal
    let couponId = $(this).data('id')
    let = couponUrl = '/admin/couponStatus/' + couponId
    $('.modal-footer #couponConfirmBTN').attr('href', couponUrl)

  });
</script>

