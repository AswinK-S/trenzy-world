<script>
  $('.modalBTN').click(function () {
    //user modal
    var userId = $(this).data('id');
    var routeURL = '/admin/userStatus/' + userId
    $(".modal-footer #confirmBTN").attr('href', routeURL);

    //category  modal
    var categoryId = $(this).data('id');
    var catrouteURL = '/admin/categoryStatus/' + categoryId
    $(".modal-footer #catconfirmBTN").attr('href', catrouteURL);

    //product modal
    var productId = $(this).data('id');
    var prodrouteURL = '/admin/productStatus/' + productId
    $(".modal-footer #prodConfirmBTN").attr('href', prodrouteURL);
  });
</script>

